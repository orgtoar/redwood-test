{
  "name": "storybook-framework-redwoodjs-vite",
  "type": "module",
  "version": "7.0.0",
  "description": "Storybook for RedwoodJS with Vite",
  "keywords": [
    "Storybook",
    "RedwoodJS",
    "React",
    "Vite"
  ],
  "bugs": {
    "url": "https://github.com/redwoodjs/redwood/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/storybook"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    },
    "./dist/mocks/MockRouter": "./dist/mocks/MockRouter.js",
    "./dist/preview.js": "./dist/preview.js",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "tsx build.ts && yarn build:types",
    "build:types": "tsc --build --verbose",
    "build:pack": "yarn pack -o storybook-framework-redwoodjs-vite.tgz"
  },
  "dependencies": {
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.3.1",
    "@rollup/pluginutils": "^5.0.2",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/builder-vite": "7.6.17",
    "@storybook/react": "7.6.17",
    "magic-string": "^0.30.0",
    "react-docgen": "^7.0.0",
    "unplugin-auto-import": "^0.17.5"
  },
  "devDependencies": {
    "@types/node": "20.11.25",
    "tsx": "4.7.1",
    "typescript": "5.4.3",
    "vite": "5.1.6"
  },
  "peerDependencies": {
    "@redwoodjs/project-config": "workspace:*",
    "@redwoodjs/router": "workspace:*",
    "react": "18.3.0-canary-a870b2d54-20240314",
    "react-dom": "18.3.0-canary-a870b2d54-20240314",
    "storybook": "7.6.17"
  },
  "engines": {
    "node": "=20.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
